{
    "manifest_version": 2,
    "name": "The Popup Terminator",
    "description": "terminates all popup windows",
    "version": "0.1",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["http://*/*", "tabs", "debugger"],
    "browser_action": {
        "default_icon":"icon.png"
    }
}
